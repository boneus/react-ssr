{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Simple SSR (without Next.JS) with React.JS, React Router and Redux",
  "main": "index.js",
  "scripts": {
    "dev:build:server": "webpack --config webpack.server.js --watch",
    "dev:server": "nodemon --watch build --exec node build/server.js",
    "dev:build:client": "webpack --config webpack.client.js --watch",
    "dev": "concurrently \"npm:dev:*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boneus/react-ssr.git"
  },
  "author": "Stanislav Tiptsov <4terion@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boneus/react-ssr/issues"
  },
  "homepage": "https://github.com/boneus/react-ssr#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "axios": "^0.25.0",
    "express": "^4.17.2",
    "express-http-proxy": "^1.6.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "serialize-javascript": "^6.0.0"
  }
}
